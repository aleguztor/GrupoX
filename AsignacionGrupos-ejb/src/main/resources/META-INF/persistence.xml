<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="AsignacionGrupos" transaction-type="JTA">
	<jta-data-source>java:jboss/datasources/sii</jta-data-source>
		<class>Entidades.Alumno</class>
		<class>Entidades.Asignatura</class>
		<class>Entidades.Centro</class>
		<class>Entidades.Encuesta</class>
		<class>Entidades.Expediente</class>
		<class>Entidades.Clase</class>
		<class>Entidades.Grupo</class>
		<class>Entidades.Matricula</class>
		<class>Entidades.Titulacion</class>
		<class>Entidades.Grupos_Por_Asignatura</class>
		<class>Entidades.Optativa</class>
		<class>Entidades.Asignaturas_matriculas</class>
		<shared-cache-mode>NONE</shared-cache-mode>
		<properties>
			<property name="javax.persistence.schema-generation.database.action" value="create"/>
		</properties>
	</persistence-unit>
</persistence>
